fn add(a: f32, b: f32) -> f32 {
    return a + b
}

fn fib(num: i32) -> i32 {
    if num < 2 {
        return num
    }
    return fib(num - 1) + fib(num - 2)
}

fn main() {
    let string = "Hello, World!"
    let i = 1
    let b = 2.5
    print_float(add(b, 3.25))
    print_str(string)
    loop {
        print_int(fib(i))
        if i >= 10 {
            break
        }
        i = i + 1
    }
}